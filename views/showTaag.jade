extends layout

block content
    // Check and see if taag is populated 
    unless taag == null 
        #displayTaag
            h3.color-4=taag.Title

            p 
                | Type: 
                | #{taag.Type}

            p 
                | Description:  
                |#{taag.Description}
    else
        h2.color-4 Sorry, No Taag Found